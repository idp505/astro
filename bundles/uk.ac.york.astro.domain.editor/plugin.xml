<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated astro -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="astro.presentation.AstroModelWizardID"
            name="%_UI_AstroModelWizard_label"
            class="astro.presentation.AstroModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/AstroModelFile.gif">
         <description>%_UI_AstroModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated astro -->
      <editor
            id="astro.presentation.AstroEditorID"
            name="%_UI_AstroEditor_label"
            icon="icons/full/obj16/AstroModelFile.gif"
            extensions="astro"
            class="astro.presentation.AstroEditor"
            contributorClass="astro.presentation.AstroActionBarContributor">
      </editor>
   </extension>

</plugin>
